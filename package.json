{
  "name": "dbl-utils",
  "version": "1.0.10",
  "description": "Utilities for dbl, adba and others projects",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "types": "./dist/types/index.d.ts",
      "source": "./src/index.ts"
    },
    "./ts/*": {
      "types": "./dist/types/index.d.ts",
      "source": "./src"
    },
    "./esm/*": "./dist/esm/*",
    "./cjs/*": "./dist/cjs/*",
    "./esm/event-handler": "./dist/esm/event-handler.js",
    "./esm/extract-react-node-text": "./dist/esm/extract-react-node-text.js",
    "./esm/fetch-queue": "./dist/esm/fetch-queue.js",
    "./esm/flat": "./dist/esm/flat.js",
    "./esm/format-value": "./dist/esm/format-value.js",
    "./esm/i18n": "./dist/esm/i18n.js",
    "./esm/index": "./dist/esm/index.js",
    "./esm/object-mutation": "./dist/esm/object-mutation.js",
    "./esm/purge-css-cli": "./dist/esm/purge-css-cli.js",
    "./esm/purge-css": "./dist/esm/purge-css.js",
    "./esm/resolve-refs-try2": "./dist/esm/resolve-refs-try2.js",
    "./esm/resolve-refs": "./dist/esm/resolve-refs.js",
    "./esm/utils": "./dist/esm/utils.js",
    "./cjs/event-handler": "./dist/cjs/event-handler.js",
    "./cjs/extract-react-node-text": "./dist/cjs/extract-react-node-text.js",
    "./cjs/fetch-queue": "./dist/cjs/fetch-queue.js",
    "./cjs/flat": "./dist/cjs/flat.js",
    "./cjs/format-value": "./dist/cjs/format-value.js",
    "./cjs/i18n": "./dist/cjs/i18n.js",
    "./cjs/index": "./dist/cjs/index.js",
    "./cjs/object-mutation": "./dist/cjs/object-mutation.js",
    "./cjs/purge-css-cli": "./dist/cjs/purge-css-cli.js",
    "./cjs/purge-css": "./dist/cjs/purge-css.js",
    "./cjs/resolve-refs-try2": "./dist/cjs/resolve-refs-try2.js",
    "./cjs/resolve-refs": "./dist/cjs/resolve-refs.js",
    "./cjs/utils": "./dist/cjs/utils.js"
  },
  "directories": {
    "ts": "src",
    "esm": "dist/esm",
    "cjs": "dist/cjs"
  },
  "scripts": {
    "build:cjs": "tsc -p tsconfig.build.json",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build": "jest && rm -rf dist/* && npm run build:cjs && npm run build:esm && typedoc",
    "doc": "typedoc",
    "test": "jest",
    "test:e2e": "jest --config ./jest.config.e2e.ts --forceExit --verbose --detectOpenHandles --coverage --runInBand"
  },
  "author": "joneldiablo",
  "license": "ISC",
  "peerDependencies": {
    "react": "^18.3.1",
    "prettier": "^3.5.3",
    "purgecss": "^7.0.2"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    }
  },
  "dependencies": {
    "change-case-all": "^2.1.0",
    "chroma-js": "^3.1.2",
    "js-md5": "^0.8.3",
    "json2csv": "^6.0.0-alpha.2",
    "moment": "^2.30.1",
    "numeral": "^2.0.6",
    "uuid": "^11.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/preset-env": "^7.26.0",
    "@types/change-case": "^2.3.5",
    "@types/chroma-js": "^2.4.4",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/flat": "^5.0.5",
    "@types/jest": "^29.5.14",
    "@types/json2csv": "^5.0.7",
    "@types/lodash": "^4.17.12",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.7.9",
    "@types/numeral": "^2.0.5",
    "@types/react": "^18.3.12",
    "@types/uuid": "^10.0.0",
    "babel-jest": "^29.7.0",
    "jest": "^29.7.0",
    "sqlite3": "^5.1.7",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typedoc": "^0.26.11",
    "typescript": "^5.6.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/joneldiablo/dbl-utils.git"
  },
  "keywords": [
    "NodeJS",
    "TypeScript",
    "JavaScript",
    "ESM",
    "CommonJS",
    "frontend",
    "backend",
    "express",
    "react"
  ]
}